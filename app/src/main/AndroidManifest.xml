<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exomatik.desacenranabaru">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
        >
        <activity android:name=".base.BaseActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".ui.main.MainAct"
            android:screenOrientation="portrait"
            />
        <activity android:name=".ui.location.PickLocationAct"
            android:screenOrientation="portrait"
            />
        <activity android:name=".ui.location.LocationAct"
            android:screenOrientation="portrait"
            />
        <activity android:name=".ui.proker.DetailProkerAct"
            android:screenOrientation="portrait"
            />
        <activity android:name=".ui.wisata.DetailWisataAct"
            android:screenOrientation="portrait"
            />
        <activity android:name=".ui.image.DetailFotoAct"
            android:screenOrientation="portrait"
            />
        <activity android:name=".ui.splash.SplashAct"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
    </application>

</manifest>