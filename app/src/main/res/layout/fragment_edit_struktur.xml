        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/etKontakSekertarisDesa"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Kontak Sekertaris Desa"
            android:textColorHint="@color/gray2"
            app:errorEnabled="true"
            app:errorTextAppearance="@style/error_appearance"
            android:layout_marginTop="@dimen/margin_standard"
            >

            <com.google.android.material.textfield.TextInputEditText
                style="@style/textStandardWhite"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="textStart"
                android:textColor="@color/colorBlack"
                android:imeOptions="actionNext"
                android:inputType="number"
                />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/etNamaDusunMalaka"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Nama Dusun Malaka"
            android:textColorHint="@color/gray2"
            app:errorEnabled="true"
            app:errorTextAppearance="@style/error_appearance"
            android:layout_marginTop="@dimen/margin_standard"
            >

            <com.google.android.material.textfield.TextInputEditText
                style="@style/textStandardWhite"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="textStart"
                android:textColor="@color/colorBlack"
                android:imeOptions="actionNext"
                android:inputType="text"
                />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/etKontakDusunMalaka"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Kontak Kepala Dusun Malaka"
            android:textColorHint="@color/gray2"
            app:errorEnabled="true"
            app:errorTextAppearance="@style/error_appearance"
            android:layout_marginTop="@dimen/margin_standard"
            >

            <com.google.android.material.textfield.TextInputEditText
                style="@style/textStandardWhite"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="textStart"
                android:textColor="@color/colorBlack"
                android:imeOptions="actionNext"
                android:inputType="number"
                />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/etNamaDusunArokke"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Nama Dusun Arokke"
